{
  "name": "superior",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "concurrently": "^5.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@types/react-router": "^5.1.3",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sslify": "^1.2.0",
    "fs": "0.0.1-security",
    "gridfs-stream": "^1.1.1",
    "if-env": "^1.0.4",
    "method-override": "^3.0.0",
    "mongodb": "^3.4.0",
    "mongoose": "^5.8.1",
    "mongoose-gridfs": "^1.2.14",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.1",
    "nodemailer": "^6.4.2",
    "path": "^0.12.7",
    "process-env": "^1.1.0",
    "serve-favicon": "^2.5.0"
  },
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacquezdaniel/superior-replacement.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacquezdaniel/superior-replacement/issues"
  },
  "homepage": "https://github.com/jacquezdaniel/superior-replacement#readme"
}
